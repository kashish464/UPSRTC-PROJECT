jQuery(document).ready(function () {
    window.FPConfig = { delay: 0, ignoreKeywords: [], maxRPS: 3, hoverDelay: 50 };
    var $window = $(window);
    $window.on("load", function () {
        $("#status").fadeOut();
        $("#preloader").delay(350).fadeOut("slow");
        $("body").delay(350).css({ overflow: "visible" });
        new WOW().init();
        $(".grid-masonry").masonry({ itemSelector: ".grid-item", columnWidth: ".grid-item" });
        $("#status").fadeOut();
        $("#preloader").delay(350).fadeOut("slow");
    });
    $window.on("scroll", function () {
        if ($(".navbar-default").add(".navbar-inverse").offset().top > 50) {
            $(".reveal-menu-home").addClass("sticky-nav");
            $(".reveal-menu-blog").addClass("sticky-nav-white");
        } else {
            $(".reveal-menu-home").removeClass("sticky-nav");
            $(".reveal-menu-blog").removeClass("sticky-nav-white");
        }
    });
 
    $window
        .on("resize", function () {
            var bodyheight = $(this).height();
            $("#mt_banner").height(bodyheight);
        })
        .resize();
    try {
        $(".counter-box").appear(function () {
            $(".timer").countTo();
        });
    } catch (err) {
        console.log(err.message);
    }
	
    $(document).ready(function () {
		$( "#menu span" ).remove( ".ui-accordion-header-icon" );
		$( "#menu ul" ).removeAttr( "class" );
		$(window).scroll(function () {
			if ($(this).scrollTop() > 350) {
            $(".othermenu").addClass("menuFxd");
        } else {
            $(".othermenu").removeClass("menuFxd");
        }
    });
        $(window).scroll(function () {
            if ($(this).scrollTop() > 50) {
                $("#back-to-top").fadeIn();
            } else {
                $("#back-to-top").fadeOut();
            }
        });
		
        $("#back-to-top").click(function () {
            $("#back-to-top").tooltip("hide");
            $("body,html").animate({ scrollTop: 0 }, 800);
            return false;
        });
        $("#back-to-top").tooltip("show");
    });
  });
     
